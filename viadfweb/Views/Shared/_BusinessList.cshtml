@model IEnumerable<viadflib.Business>

@using viadflib

@if (Model.Count() > 0)
{
    <table class="businessList">        
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(Utils.Capitalize(item.Name), "Business", "Home", new { name = item.SeoName, id = item.ID }, null)
                </td>              
            </tr>
        }
    </table>
}

